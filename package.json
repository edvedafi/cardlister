{
  "dependencies": {
    "@u4/opencv4nodejs": "^6.5.1",
    "csv-writer": "^1.6.0",
    "firebase": "^9.19.1",
    "firebase-admin": "^11.6.0",
    "terminal-image": "^1.0.0",
    "zx": "^7.2.1"
  },
  "scripts": {
    "process": "node trim.mjs",
    "install_arm64": "CXXFLAGS=\"-std=c++17 -Wno-c++11-narrowing\" && build-opencv --version 4.7.0_4 --flag=\"-DCMAKE_SYSTEM_PROCESSOR=arm64 -DCMAKE_OSX_ARCHITECTURES=arm64\" build",
    "install_4.5.5_cuda": "build-opencv --version 4.5.5 --flags=\"-DWITH_CUDA=ON -DWITH_CUDNN=ON -DOPENCV_DNN_CUDA=ON -DCUDA_FAST_MATH=ON\" build",
    "do-install": "build-opencv build"
  },
  "opencv4nodejs": {
    "disableAutoBuild": 1,
    "opencvIncludeDir": "/usr/local/Cellar/opencv/4.7.0_4/include",
    "opencvLibDir": "/usr/local/Cellar/opencv/4.7.0_4/lib",
    "opencvBinDir": "/usr/local/Cellar/opencv/4.7.0_4/bin"
  }
}
