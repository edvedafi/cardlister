    "install_arm64": "CXXFLAGS=\"-std=c++17 -Wno-c++11-narrowing\" && build-opencv --version 4.7.0_4 --flag=\"-DCMAKE_SYSTEM_PROCESSOR=arm64 -DCMAKE_OSX_ARCHITECTURES=arm64\" build",
    "install_4.5.5_cuda": "build-opencv --version 4.5.5 --flags=\"-DWITH_CUDA=ON -DWITH_CUDNN=ON -DOPENCV_DNN_CUDA=ON -DCUDA_FAST_MATH=ON\" build",
    "do-install": "build-opencv build",
    "login": "gcloud auth application-default login; gcloud auth application-default set-quota-project hofdb-2038e",
    "install-python-deps": "pip3 install -r python.requirements.txt",
    "install-spacy-models": "python3 -m spacy download en_core_web_md",
